{
  "name": "@thetribe/generator-project",
  "version": "0.4.3",
  "keywords": [
    "yeoman-generator"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "binascii": "^0.0.2",
    "crypto-random-string": "^3.3.1",
    "ejs": "^3.1.9",
    "indent-tag": "^0.4.0",
    "ramda": "^0.29.0",
    "tslib": "^2.5.0",
    "yaml": "^2.2.2",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@thetribe/eslint-config-typescript": "^0.4.1",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.16.3",
    "@types/ramda": "^0.29.0",
    "@types/xml2js": "^0.4.11",
    "@types/yeoman-generator": "^5.2.10",
    "@types/yeoman-test": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "execa": "^5.1.1",
    "fast-plist": "^0.1.3",
    "glob": "^10.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "xml2js": "^0.5.0",
    "yeoman-environment": "^3.2.0",
    "yeoman-test": "^5.1.0"
  },
  "scripts": {
    "build": "./scripts/build.mjs",
    "lint": "eslint --ext ts generators utils",
    "test": "yarn test:utils && yarn test:generators",
    "test:generators": "node --test --test-concurrency=1 --loader ts-node/esm --experimental-specifier-resolution=node $(glob --ignore='**/templates/**' 'generators/**/*.test.ts')",
    "test:utils": "node --test --loader ts-node/esm --experimental-specifier-resolution=node $(glob 'utils/**/*.test.ts')",
    "test:utils:coverage": "yarn test:utils --coverage --collectCoverageFrom 'utils/**/*'"
  },
  "packageManager": "yarn@3.5.1"
}
