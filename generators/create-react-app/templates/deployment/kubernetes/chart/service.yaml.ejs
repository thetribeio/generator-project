apiVersion: v1
kind: Service
metadata:
  name: {{ include "<%= projectName %>.fullname" . }}-<%= packageName %>
  labels:
    app.kubernetes.io/component: <%= packageName %>
    {{- include "<%= projectName %>.labels" . | nindent 4 }}
spec:
  selector:
    app.kubernetes.io/component: <%= packageName %>
    {{- include "<%= projectName %>.selectorLabels" . | nindent 4 }}
  ports:
    - protocol: TCP
      port: 80
      targetPort: 80
      name: http
