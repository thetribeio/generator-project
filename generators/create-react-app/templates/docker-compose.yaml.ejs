services:
    <%= packageName %>:
        build:
            context: <%= packagePath %>/docker
            args:
                UID: ${UID:-1000}
        environment:
            DISABLE_ESLINT_PLUGIN: 'true'
            WDS_SOCKET_PATH: <%= httpPath %>sockjs-node
        volumes:
            - ./<%= packagePath %>:/usr/src/project/<%= packagePath %>
