apiVersion: v1
kind: Secret
metadata:
  name: {{ include "<%= projectName %>.fullname" . }}-<%= packageName %>-app
  labels:
    app.kubernetes.io/component: <%= packageName %>
    {{- include "<%= projectName %>.labels" . | nindent 4 }}
type: Opaque
stringData:
  secret: {{ .Values.<%= varName(packageName) %>.app.secret | quote }}
