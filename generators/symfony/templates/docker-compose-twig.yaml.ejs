version: '2.4'

services:
    <%= packageName %>-node:
        build:
            context: <%= packagePath %>/docker/node
            args:
                UID: ${UID:-1000}
        command: yarn start
        depends_on:
            - <%= packageName %>-nginx
        volumes_from:
            - <%= packageName %>-php
        ports:
            - 8080:8080
