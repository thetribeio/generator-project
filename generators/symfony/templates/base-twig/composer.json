{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "doctrine/doctrine-bundle": "^2.7.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/orm": "^2.13.4",
        "sensio/framework-extra-bundle": "^6.2.9",
        "sentry/sentry-symfony": "^4.5.0",
        "symfony/asset": "^6.2.0",
        "symfony/console": "^6.2.1",
        "symfony/expression-language": "^6.2.0",
        "symfony/flex": "^2.2.3",
        "symfony/form": "^6.2.0",
        "symfony/framework-bundle": "^6.2.1",
        "symfony/http-client": "^6.2.0",
        "symfony/intl": "^6.2.0",
        "symfony/process": "^6.2.0",
        "symfony/security-bundle": "^6.2.0",
        "symfony/translation": "^6.2.0",
        "symfony/twig-bundle": "^6.2.0",
        "symfony/validator": "^6.2.0",
        "symfony/web-link": "^6.2.0",
        "symfony/yaml": "^6.2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13.0",
        "phpstan/extension-installer": "^1.2.0",
        "phpstan/phpstan": "^1.9.2",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-doctrine": "^1.3.25",
        "phpstan/phpstan-symfony": "^1.2.16",
        "symfony/browser-kit": "^6.2.0",
        "symfony/css-selector": "^6.2.0",
        "symfony/debug-bundle": "^6.2.1",
        "symfony/phpunit-bridge": "^6.2.0",
        "symfony/stopwatch": "^6.2.0",
        "symfony/web-profiler-bundle": "^6.2.1",
        "thetribe/coding-standard": "^0.3.0"
    },
    "config": {
        "platform": {
            "php": "8.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "9.99.100",
        "ralouphie/getallheaders": "3.0.3",
        "symfony/polyfill-ctype": "1.25.0",
        "symfony/polyfill-iconv": "1.25.0",
        "symfony/polyfill-intl-grapheme": "1.25.0",
        "symfony/polyfill-intl-icu": "1.25.0",
        "symfony/polyfill-intl-idn": "1.25.0",
        "symfony/polyfill-intl-normalizer": "1.25.0",
        "symfony/polyfill-mbstring": "1.25.0",
        "symfony/polyfill-php56": "1.20.0",
        "symfony/polyfill-php70": "1.20.0",
        "symfony/polyfill-php71": "1.20.0",
        "symfony/polyfill-php72": "1.25.0",
        "symfony/polyfill-php73": "1.25.0",
        "symfony/polyfill-php80": "1.25.0",
        "symfony/polyfill-php81": "1.25.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3.0"
        }
    }
}
