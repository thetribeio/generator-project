{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "doctrine/doctrine-bundle": "^2.2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0.2",
        "doctrine/orm": "^2.8.2",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^3.5.3",
        "symfony/console": "^5.2.5",
        "symfony/expression-language": "^5.2.4",
        "symfony/flex": "^1.12.2",
        "symfony/framework-bundle": "^5.2.5",
        "symfony/http-client": "^5.2.4",
        "symfony/intl": "^5.2.4",
        "symfony/process": "^5.2.4",
        "symfony/security-bundle": "^5.2.5",
        "symfony/translation": "^5.2.5",
        "symfony/validator": "^5.2.5",
        "symfony/web-link": "^5.2.5",
        "symfony/yaml": "^5.2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.82",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-doctrine": "^0.12.33",
        "phpstan/phpstan-symfony": "^0.12.21",
        "symfony/browser-kit": "^5.2",
        "symfony/css-selector": "^5.2",
        "symfony/debug-bundle": "^5.2.4",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/web-profiler-bundle": "^5.2",
        "thetribe/coding-standard": "^0.1.4"
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "9.99.99",
        "ralouphie/getallheaders": "3.0.3",
        "symfony/polyfill-ctype": "1.12.0",
        "symfony/polyfill-iconv": "1.12.0",
        "symfony/polyfill-intl-icu": "1.12.0",
        "symfony/polyfill-intl-idn": "1.12.0",
        "symfony/polyfill-mbstring": "1.12.0",
        "symfony/polyfill-php56": "1.12.0",
        "symfony/polyfill-php70": "1.12.0",
        "symfony/polyfill-php71": "1.12.0",
        "symfony/polyfill-php72": "1.12.0",
        "symfony/polyfill-php73": "1.12.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
