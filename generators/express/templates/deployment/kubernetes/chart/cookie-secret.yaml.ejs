apiVersion: v1
kind: Secret
metadata:
  name: {{ include "<%= projectName %>.fullname" . }}-<%= packageName %>-cookie
  labels:
    app.kubernetes.io/component: <%= packageName %>
    {{- include "<%= projectName %>.labels" . | nindent 4 }}
type: Opaque
stringData:
  secret: {{ .Values.<%= varify(packageName) %>.cookie.secret | quote }}
